<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="375.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.app.registration.SecondPageController"
            fx:id="root">
    <children>
        <VBox fx:id="box" alignment="CENTER" fillWidth="false" spacing="5.0" AnchorPane.leftAnchor="30.0"
              AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onClickLoad" text="Загрузить файл"/>
            </children>
        </VBox>
        <ButtonBar prefHeight="40.0" prefWidth="180.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20">
            <buttons>
                <Button mnemonicParsing="false" onAction="#onCancel" text="Отменить">
                    <font>
                        <Font name="MS Outlook" size="12.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#skip" text="Пропустить">
                    <font>
                        <Font name="MS Outlook" size="12.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#onClickNext" text="Отправить">
                    <font>
                        <Font name="MS Outlook" size="12.0"/>
                    </font>
                </Button>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
