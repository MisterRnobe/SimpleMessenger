<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="root" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.menu.CreateGroupChannelController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: #eee;">
         <children>
            <Circle fill="#ffffff" radius="40.0" stroke="BLACK" strokeType="INSIDE" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" fx:id="avatarSelector" onMouseClicked="#selectAvatar"/>
            <VBox AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="27.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Название :">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Text>
                  <TextField fx:id="title" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="empty" prefHeight="250.0" prefWidth="300.0" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #eee;">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onClick" text="Создать" />
            <Button layoutX="264.0" layoutY="18.0" mnemonicParsing="false" text="Отменить" />
        </buttons>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
