<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<VBox style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.dialog.info.AddUsersToGroupController" fx:id="root">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="300.0" fx:id="wrapper"/>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #eee;">
        <buttons>
          <Button mnemonicParsing="false" text="Добавить" onAction="#onOk"/>
            <Button layoutX="235.0" layoutY="18.0" mnemonicParsing="false" text="Отменить" onAction="#onCancel"/>
        </buttons>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
